{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "center": true,
        "decorations": true,
        "dragDropEnabled": true,
        "fullscreen": false,
        "height": 800,
        "minHeight": 600,
        "minWidth": 800,
        "resizable": true,
        "shadow": true,
        "title": "Metis",
        "transparent": false,
        "width": 1200
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "io.colliery.metis",
  "productName": "Metis",
  "version": "0.6.1"
}